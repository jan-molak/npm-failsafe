{
  "name": "npm-failsafe",
  "version": "0.0.0-development",
  "description": "Executes a sequence of npm scripts.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "bin": {
    "failsafe": "bin/failsafe"
  },
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint --ext ts --config .eslintrc.yml .",
    "lint:fix": "npm run lint -- --fix",
    "test": "mocha 'spec/*.spec.ts'",
    "compile": "tsc --project tsconfig.json",
    "commit": "git-cz",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jan-molak/npm-failsafe.git"
  },
  "keywords": [
    "cli",
    "npm",
    "script",
    "scripts",
    "ci"
  ],
  "author": "Jan Molak <jan.molak@smartcodeltd.co.uk>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jan-molak/npm-failsafe/issues"
  },
  "homepage": "https://github.com/jan-molak/npm-failsafe",
  "dependencies": {},
  "engines": {
    "node": ">= 6.12.x",
    "npm": ">= 3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^9.1.0",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.31.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "eslint-plugin-unused-imports": "^2.0.0",
    "mocha": "^8.2.1",
    "mocha-testdata": "^1.2.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^20.1.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.0.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
