{
  "name": "npm-failsafe",
  "version": "0.0.0-development",
  "description": "Executes a sequence of npm scripts.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "bin": {
    "failsafe": "bin/failsafe"
  },
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint --ext ts --config .eslintrc.yml .",
    "lint:fix": "npm run lint -- --fix",
    "test": "mocha 'spec/*.spec.ts'",
    "compile": "tsc --project tsconfig.json",
    "commit": "git-cz",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jan-molak/npm-failsafe.git"
  },
  "keywords": [
    "cli",
    "npm",
    "script",
    "scripts",
    "ci"
  ],
  "author": "Jan Molak <jan.molak@smartcodeltd.co.uk>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jan-molak/npm-failsafe/issues"
  },
  "homepage": "https://github.com/jan-molak/npm-failsafe",
  "dependencies": {},
  "engines": {
    "node": "^14 || ^16 || ^18",
    "npm": "^6 || ^7 || ^8 || ^9"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.18.38",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.43.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "mocha": "^10.2.0",
    "mocha-testdata": "^1.2.0",
    "rimraf": "^5.0.1",
    "semantic-release": "^21.0.6",
    "supports-color": "^9.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
