{
  "name": "npm-failsafe",
  "version": "0.0.0-development",
  "description": "Executes a sequence of npm scripts.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "bin": {
    "failsafe": "bin/failsafe"
  },
  "scripts": {
    "clean": "rimraf lib",
    "lint": "tslint --project ./tsconfig.json --config ./tslint.json --format prose",
    "test": "mocha 'spec/*.spec.ts'",
    "compile": "tsc --project tsconfig.json",
    "commit": "git-cz",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jan-molak/npm-failsafe.git"
  },
  "keywords": [
    "cli",
    "npm",
    "script",
    "scripts",
    "ci"
  ],
  "author": "Jan Molak <jan.molak@smartcodeltd.co.uk>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jan-molak/npm-failsafe/issues"
  },
  "homepage": "https://github.com/jan-molak/npm-failsafe",
  "dependencies": {},
  "engines": {
    "node": ">= 6.12.x",
    "npm": ">= 3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^8.0.3",
    "@types/node": "^16.10.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "lodash": "^4.17.20",
    "mocha": "^8.2.1",
    "mocha-testdata": "^1.2.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.2.2",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^4.0.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
